{% extends "base" %}

{% block content %}
<table class="table">
    <thead>
    <tr>
        <th scope="col">Name</th>
        <th scope="col">Link</th>
    </tr>
    </thead>
    <tbody>
    {% for link in data %}
        <tr>
            <td scope="row">{{ link[0] }}</td>
            <td>
                <a href="{{ link[1] }}">{{ link[1] }}</a>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>
{% endblock content %}